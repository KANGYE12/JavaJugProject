		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void tableEmptyTest1() {
			m1.fill(Left);
			m1.fill(Right);
			precond(5, m1.getCapacity(Left));
			precond(5, m1.getContent(Left));
			precond(2, m1.getCapacity(Right));
			precond(2, m1.getContent(Right));
			m1.empty(Left);
			assertAll("tableEmptyTest1",
					() -> assertEquals(5, m1.getCapacity(Left), "\n> Error: m5522.empty(Left): Capacity-J1:"),
					() -> assertEquals(0, m1.getContent(Left), "\n> Error: m5522.empty(Left): Content-J1:"),
					() -> assertEquals(2, m1.getCapacity(Right), "\n> Error: m5522.empty(Left): Capacity-J2:"),
					() -> assertEquals(2, m1.getContent(Right), "\n> Error: m5522.empty(Left): Content-J2:"));
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void tableEmptyTest2() {
			m1.fill(Left);
			m1.fill(Right);
			precond(5, m1.getCapacity(Left));
			precond(5, m1.getContent(Left));
			precond(2, m1.getCapacity(Right));
			precond(2, m1.getContent(Right));
			m1.empty(Right);
			assertAll("tableEmptyTest2",
