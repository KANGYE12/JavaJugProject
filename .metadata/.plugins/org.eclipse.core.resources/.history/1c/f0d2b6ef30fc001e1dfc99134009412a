		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void tablePourFromTest1() {
			m1.fill(Right);
			precond(5, m1.getCapacity(Left));
			precond(0, m1.getContent(Left));
			precond(2, m1.getCapacity(Right));
			precond(2, m1.getContent(Right));
			m1.pourFrom(Right);
			assertAll("tablePourFromTest1",
					() -> assertEquals(5, m1.getCapacity(Left), "\n> Error: m5022.pourFrom(Right): Capacity-J1:"),
					() -> assertEquals(2, m1.getContent(Left), "\n> Error: m5022.pourFrom(Right): Content-J1:"),
					() -> assertEquals(2, m1.getCapacity(Right), "\n> Error: m5022.pourFrom(Right): Capacity-J2:"),
					() -> assertEquals(0, m1.getContent(Right), "\n> Error: m5022.pourFrom(Right): Content-J2:"));
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void tablePourFromTest2() {
			m1.fill(Left);
			precond(5, m1.getCapacity(Left));
			precond(5, m1.getContent(Left));
			precond(2, m1.getCapacity(Right));
			precond(0, m1.getContent(Right));
			m1.pourFrom(Left);
			assertAll("tablePourFromTest2",
					() -> assertEquals(5, m1.getCapacity(Left),"\n> Error: m5520.pourFrom(Left): Capacity-J1:"),
					() -> assertEquals(3, m1.getContent(Left), "\n> Error: m5520.pourFrom(Left): Content-J1:"),
					() -> assertEquals(2, m1.getCapacity(Right), "\n> Error: m5520.pourFrom(Left): Capacity-J2:"),
					() -> assertEquals(2, m1.getContent(Right), "\n> Error: m5520.pourFrom(Left): Content-J2:"));
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void tablePourFromTest3() {
			m1.fill(Right);
			precond(5, m1.getCapacity(Left));
			precond(0, m1.getContent(Left));
			precond(2, m1.getCapacity(Right));
			precond(2, m1.getContent(Right));
