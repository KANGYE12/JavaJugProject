		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void tableToStringTest1() {
			precond(5, m1.getCapacity(Left));
			precond(0, m1.getContent(Left));
			precond(2, m1.getCapacity(Right));
			precond(0, m1.getContent(Right));
			assertEquals(normalize("T(J(5,0),J(2,0))"), normalize(m1.toString()),
					"\n> Error: m5020.toString():");
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void tableToStringTest2() {
			m1.fill(Left);
			m1.fill(Right);
			precond(5, m1.getCapacity(Left));
			precond(5, m1.getContent(Left));
			precond(2, m1.getCapacity(Right));
			precond(2, m1.getContent(Right));
			assertEquals(normalize("T(J(5,5),J(2,2))"), normalize(m1.toString()),
					"\n> Error: m5522.toString():");
		}
		//------------------------------------------------------------------
	}
