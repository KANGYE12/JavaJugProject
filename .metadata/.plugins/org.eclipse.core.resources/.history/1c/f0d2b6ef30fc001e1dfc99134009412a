	@Nested
	@TestInstance(TestInstance.Lifecycle.PER_CLASS) 
	public class JUnitTestJug {
		private Jug j5;
		@BeforeAll
		public void beforeClass() {
			// Code executed before the first test method
			System.out.println("Start of Jug JUnit Test");
		}
		@AfterAll
		public void afterClass() {
			// Code executed after the last test method
			System.out.println("End of Jug JUnit Test");
		}
		@BeforeEach
		public void setUp() {
			// Code executed before each test
			j5 = new Jug(5);
		}
		@AfterEach
		public void tearDown() {
			// Code executed after each test
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void jugCtorTest1() {
			assertAll("jugCtorTest1",
			() -> assertEquals(5, j5.capacity(), "\n> Error: new Jug(5): Capacity:"),
			() -> assertEquals(0, j5.content(), "\n> Error: new Jug(5): Content:"));
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void jugCtorTestX1() {
			Exception exception = 
						assertThrows(RuntimeException.class, () -> new Jug(-1), "\n> Error: new Jug(-1): no exception thrown");
			assertEquals(null, exception.getMessage());
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void jugFillTest1() {
			precond(5, j5.capacity());
			precond(0, j5.content());
			j5.fill();
			assertAll("jugFillTest1", 
					() -> assertEquals(5, j5.capacity(), "\n> Error: j50.fill(): Capacity:"),
					() -> assertEquals(5, j5.content(), "\n> Error: j50.fill(): Content:"));
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void jugEmptyTest1() {
			j5.fill();
			precond(5, j5.capacity());
			precond(5, j5.content());
			j5.empty();
			assertAll("jugEmptyTest1",
					() -> assertEquals(5, j5.capacity(), "\n> Error: j55.empty(): Capacity:"),
					() -> assertEquals(0, j5.content(), "\n> Error: j55.empty(): Content:"));
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void jugPourFromTest1() {
			Jug j2 = new Jug(2);
			j2.fill();
			precond(5, j5.capacity());
			precond(0, j5.content());
			precond(2, j2.capacity());
			precond(2, j2.content());
			j5.pourFrom(j2);
			assertAll("jugPourFromTest1",
					() -> assertEquals(5, j5.capacity(), "\n> Error: j50.pourFrom(j22): j5.Capacity:"),
					() -> assertEquals(2, j5.content(), "\n> Error: j50.pourFrom(j22): j5.Content:"),
					() -> assertEquals(2, j2.capacity(), "\n> Error: j50.pourFrom(j22): j2.Capacity:"),
					() -> assertEquals(0, j2.content(), "\n> Error: j50.pourFrom(j22): j2.Content:"));
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void jugPourFromTest2() {
			Jug j2 = new Jug(2);
			j5.fill();
			precond(5, j5.capacity());
			precond(5, j5.content());
			precond(2, j2.capacity());
			precond(0, j2.content());
			j2.pourFrom(j5);
			assertAll("jugPourFromTest2",
					() -> assertEquals(5, j5.capacity(), "\n> Error: j20.pourFrom(j55): j5.Capacity:"),
					() -> assertEquals(3, j5.content(), "\n> Error: j20.pourFrom(j55): j5.Content:"),
					() -> assertEquals(2, j2.capacity(), "\n> Error: j20.pourFrom(j55): j2.Capacity:"),
					() -> assertEquals(2, j2.content(), "\n> Error: j20.pourFrom(j55): j2.Content:"));
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void jugPourFromTest3() {
			Jug j2 = new Jug(2);
			j2.fill();
			precond(5, j5.capacity());
			precond(0, j5.content());
			precond(2, j2.capacity());
			precond(2, j2.content());
			j5.pourFrom(j2);
			assertAll("jugPourFromTest3",
					() -> assertEquals(5, j5.capacity(), "\n> Error: j50.pourFrom(j22): j5.Capacity:"),
					() -> assertEquals(2, j5.content(), "\n> Error: j50.pourFrom(j22): j5.Content:"),
					() -> assertEquals(2, j2.capacity(), "\n> Error: j50.pourFrom(j22): j2.Capacity:"),
					() -> assertEquals(0, j2.content(), "\n> Error: j50.pourFrom(j22): j2.Content:"));
			j2.fill();
			precond(5, j5.capacity());
			precond(2, j5.content());
			precond(2, j2.capacity());
			precond(2, j2.content());
			j5.pourFrom(j2);
			assertAll("jugPourFromTest3",
					() -> assertEquals(5, j5.capacity(),"\n> Error: j52.pourFrom(j22): j5.Capacity:"),
					() -> assertEquals(4, j5.content(), "\n> Error: j52.pourFrom(j22): j5.Content:"),
					() -> assertEquals(2, j2.capacity(), "\n> Error: j52.pourFrom(j22): j2.Capacity:"),
					() -> assertEquals(0, j2.content(), "\n> Error: j52.pourFrom(j22): j2.Content:"));
			j2.fill();
			precond(5, j5.capacity());
			precond(4, j5.content());
			precond(2, j2.capacity());
			precond(2, j2.content());
			j5.pourFrom(j2);
			assertAll("jugPourFromTest1",
					() -> assertEquals(5, j5.capacity(), "\n> Error: j54.pourFrom(j22): j5.Capacity:"),
					() -> assertEquals(5, j5.content(), "\n> Error: j54.pourFrom(j22): j5.Content:"),
					() -> assertEquals(2, j2.capacity(), "\n> Error: j54.pourFrom(j22): j2.Capacity:"),
					() -> assertEquals(1, j2.content(), "\n> Error: j54.pourFrom(j22): j2.Content:"));
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void jugPourFromTestX1() {
			Exception exception = 
					assertThrows(RuntimeException.class, () -> j5.pourFrom(j5), "\n> Error: j5.pourFrom(j5): no exception thrown");
			assertEquals(null, exception.getMessage());
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void jugToStringTest1() {
			precond(5, j5.capacity());
			precond(0, j5.content());
			assertEquals(normalize("J(5, 0)"), normalize(j5.toString()),
					"\n> Error: j50.toString():");
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void jugToStringTest2() {
			j5.fill();
			precond(5, j5.capacity());
			precond(5, j5.content());
			assertEquals(normalize("J(5, 5)"), normalize(j5.toString()),
					"\n> Error: new j55.toString():");
		}
		//------------------------------------------------------------------
	}
