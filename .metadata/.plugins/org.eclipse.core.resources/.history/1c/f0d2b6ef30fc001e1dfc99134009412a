	@Nested
	@TestInstance(TestInstance.Lifecycle.PER_CLASS) 
	public class JUnitTestTable {
		private Table m1;
		@BeforeAll
		public void beforeClass() {
			// Code executed before the first test method
			System.out.println("Start of Table JUnit Test");
		}
		@AfterAll
		public void afterClass() {
			// Code executed after the last test method
			System.out.println("End of Table JUnit Test");
		}
		@BeforeEach
		public void setUp() {
			// Code executed before each test
			m1 = new Table(5, 2);
		}
		@AfterEach
		public void tearDown() {
			// Code executed after each test
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void tableCtorTest1() {
			assertAll("tableCtorTest1",
					() -> assertEquals(5, m1.getCapacity(Left), "\n> Error: new Table(5,2): Capacity-J1:"),
					() -> assertEquals(0, m1.getContent(Left), "\n> Error: new Table(5,2): Content-J1:"),
					() -> assertEquals(2, m1.getCapacity(Right), "\n> Error: new Table(5,2): Capacity-J2:"),
					() -> assertEquals(0, m1.getContent(Right), "\n> Error: new Table(5,2): Content-J2:"));
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void tableCtorTest2() {
			Jug j5 = new Jug(5);
			Jug j2 = new Jug(2);
			precond(5, j5.capacity());
			precond(0, j5.content());
			precond(2, j2.capacity());
			precond(0, j2.content());
			Table m2 = new Table(j5, j2);
			assertAll("tableCtorTest2",
					() -> assertEquals(5, m2.getCapacity(Left), "\n> Error: new Table(j5,j2): Capacity-J1:"),
					() -> assertEquals(0, m2.getContent(Left), "\n> Error: new Table(j5,j2): Content-J1:"),
					() -> assertEquals(2, m2.getCapacity(Right), "\n> Error: new Table(j5,j2): Capacity-J2:"),
					() -> assertEquals(0, m2.getContent(Right), "\n> Error: new Table(j5,j2): Content-J2:"));
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void tableCtorTest3() {
			Jug j5 = new Jug(5);
			Jug j2 = new Jug(2);
			Table m2 = new Table(j5, j2);
			j5.fill();
			j2.fill();
			precond(5, j5.capacity());
			precond(5, j5.content());
			precond(2, j2.capacity());
			precond(2, j2.content());
			assertAll("tableCtorTest3",
					() -> assertEquals(5, m2.getCapacity(Left), "\n> Error: new Table(j5,j2): Capacity-J1:"),
					() -> assertEquals(5, m2.getContent(Left), "\n> Error: new Table(j5,j2): Content-J1:"),
					() -> assertEquals(2, m2.getCapacity(Right), "\n> Error: new Table(j5,j2): Capacity-J2:"),
					() -> assertEquals(2, m2.getContent(Right), "\n> Error: new Table(j5,j2): Content-J2:"));
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void tableCtorTestX1() {
			Jug j5 = new Jug(5);
			Exception exception = 
					assertThrows(RuntimeException.class, () -> new Table(j5, j5), "\n> Error: new Table(j5,j5): no exception thrown");
			assertEquals(null, exception.getMessage());
		}
