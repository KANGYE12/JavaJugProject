		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void tableFillTest1() {
			precond(5, m1.getCapacity(Left));
			precond(0, m1.getContent(Left));
			precond(2, m1.getCapacity(Right));
			precond(0, m1.getContent(Right));
			m1.fill(Left);
			assertAll("tableFillTest1",
					() -> assertEquals(5, m1.getCapacity(Left), "\n> Error: m5020.fill(Left): Capacity-J1:"),
					() -> assertEquals(5, m1.getContent(Left), "\n> Error: m5020.fill(Left): Content-J1:"),
					() -> assertEquals(2, m1.getCapacity(Right), "\n> Error: m5020.fill(Left): Capacity-J2:"),
					() -> assertEquals(0, m1.getContent(Right), "\n> Error: m5020.fill(Left): Content-J2:"));
		}
		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void tableFillTest2() {
			precond(5, m1.getCapacity(Left));
			precond(0, m1.getContent(Left));
			precond(2, m1.getCapacity(Right));
			precond(0, m1.getContent(Right));
			m1.fill(Right);
			assertAll("tableFillTest2",
