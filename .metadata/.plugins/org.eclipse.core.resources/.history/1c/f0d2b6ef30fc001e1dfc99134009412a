		@Test
		@Timeout(value = 1000, unit = TimeUnit.MILLISECONDS)
		public void mainTableTest1() {
			SysOutCapture sysOutCapture = new SysOutCapture();
			sysOutCapture.sysOutCapture();
			MainTable.main(new String[]{});
			String salida = sysOutCapture.sysOutRelease();
			assertEquals(
						 normalize("T(J(7,0),J(5,5)) T(J(7,5),J(5,0)) T(J(7,5),J(5,5)) T(J(7,7),J(5,3)) T(J(7,0),J(5,3)) T(J(7,3),J(5,0)) T(J(7,3),J(5,5)) T(J(7,7),J(5,1))"),
						 normalize(salida),
